{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["./index.js", "./modules/config.js", "./modules/test.js", "./modules/VK.js"]
  },
  "plugins": [],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "systemName": "БОТ Возрастной рейтинг",
    "footer": "<div style='width: 100%;' align='center'>Бот для ВК, определяющий возрастной рейтинг материала</div>",
    "copyright": "2017",
    "navType": "vertical",
    "theme": "Cosmo",
    "linenums": true,
    "collapseSymbols": true,
    "inverseNav": true,
    "outputSourceFiles": true
  },
  "opts": {
    "template": "../ink-docstrap/template",
    "encoding": "utf8",
    "destination": "docs/js",
    "recurse": true,
    "private": true
  }
}